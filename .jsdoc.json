{
  "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["app"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "static": false,
    "sort": true
  }
}
